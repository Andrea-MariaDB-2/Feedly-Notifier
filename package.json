  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olsh/Feedly-Notifier/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olsh/Feedly-Notifier.git"
  },
  "scripts": {
    "build": "set NODE_ENV=production&& node utils/build.js",
    "start": "node utils/webserver.js",
    "sandbox": "npm run start -- --sandbox",
    "dev:chrome": "npm run sandbox -- --browser=chrome",
    "dev:firefox": "npm run sandbox -- --browser=firefox",
    "dev:opera": "npm run sandbox -- --browser=opera"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.4.1",
    "echo-loader": "^0.0.1",
    "fs-extra": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "preprocess": "^3.1.0",
    "preprocess-loader": "^0.2.2",
    "string-replace-webpack-plugin": "^0.1.3",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "write-file-webpack-plugin": "^4.2.0",
    "yargs": "^11.0.0",
    "zip-dir": "^1.0.2"
  },
  "dependencies": {
    "dompurify": "^1.0.3",
    "jquery": "^3.3.1",
    "mustache": "^2.3.0",
    "timeago": "^1.6.1",
    "webextension-polyfill": "^0.2.1"
